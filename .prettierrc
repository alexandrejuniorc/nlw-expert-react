{
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "printWidth": 100,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
        "cva",
        "clsx"
    ],
    "overrides": [
        {
            "files": ".*rc",
            "options": {
                "parser": "json"
            }
        }
    ]
}